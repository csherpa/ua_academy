<h1>Students</h1>

<%= link_to 'New Student', new_student_path, remote: true, id: "ajaxcall" %>

<div id="student-form" style="display:none;">
</div>
<%# <br> %>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Quirk</th>
      <%# <th colspan="4"></th> %>
    </tr>
  </thead>
 
  <tbody id="all-students">

    <% @students.each do |student| %> 
      <%= render student %>
    <% end %>
  
  </tbody>
</table>





