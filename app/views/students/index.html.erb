<table class="ui compact celled definition table">
  <thead>
    <tr>
      <form>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Quirk</th>
        <th>Cohort Id</th>
      </form>
    </tr>
  </thead>
  <tbody>
        <form>
          <tr> 
          <% @students.each do |single_student| %>
            <td> <%=single_student.firstname%></td>
            <td class="nav-link"><%= single_student.lastname %></td>
            <td><%= single_student.quirk%></td>
            <td><%= single_student.cohort_id %></td>   
            <td><%= link_to "Edit", edit_student_path(single_student) %></td>   
            <%# <td><%= link_to "Create", new_student_path(single_student)%>
          </tr>
            <% end %>
        </form>                 
  </tbody> 
</table>