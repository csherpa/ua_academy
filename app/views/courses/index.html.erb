<h1>Course</h1>

<%= link_to "New Course", new_course_path, remote: true, id: 'ajaxcall'%>

<div id= 'course-form' style='display:none;'></div>

<table >
  <thead>
    <tr>
      <th></th>
      <th>Title</th>
    </tr>
  </thead>

  <tbody id = 'all-courses'>
        <% @courses.each do |course| %>
          <%= render course %>   
        <% end %>
  </tbody>
</table>

