<tr id= "course-<%= course.id %>">
  <td><%= course.firstname %></td>
  <td><%= course.lastname %></td>
  <td><%= course.quirk %></td>
  <td><%= link_to 'Edit', edit_course_path(course)%></td>
  <td><%= link_to 'Remove', course_path(course), method: :delete, remote: true, data: { confirm: 'Are you sure?' } %></td>
</tr>

