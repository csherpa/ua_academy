<div class='show'>
<h1><%= @course.title%></h1>
<p>
  <strong>Title: </strong><%= @course.title%><br/>
  <strong>Hours: </strong><%= @course.hours%> 
</p>
<p>
  <strong>Cohorts: </strong>
  <%= @cohorts.each do |cohort|%>
      <%=cohort.title%>|
  <% end %>
</p>
<%= link_to 'Edit', edit_course_path(@course),class: "btn btn-outline-secondary" %> |
<%= link_to 'Back', courses_path,class: "btn btn-outline-warning" %>
</div>